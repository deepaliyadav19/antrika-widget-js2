{
  "name": "antrika-customer-portal",
  "description": "Trackwick React front end",
  "version": "2.6.158",
  "scripts": {
    "start": "webpack-dev-server --open --mode development",
    "dev-vm": "webpack-dev-server --open --mode development --host 0.0.0.0",
    "build": "webpack --progress",
    "server": "node server.js",
    "lint": "eslint --fix --ext .js --ext .jsx src test",
    "test": "jest",
    "storybook": "start-storybook",
    "deploy": "npm run build && npm run server",
    "release": "standard-version",
    "release-dry-run": "standard-version --dry-run",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "standard-version": {
    "commitUrlFormat": "https://gitlab.com/trackolap/antrika-customer-portal/commits/{{hash}}",
    "compareUrlFormat": "https://gitlab.com/trackolap/antrika-customer-portal/compare/{{currentTag}}%0D{{previousTag}}"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "Firefox ESR"
  ],
  "engines": {
    "node": "^12.0",
    "npm": "^6.0"
  },
  "dependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "@ant-design/moment-webpack-plugin": "0.0.3",
    "@babel/core": "^7.5.4",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/eslint-plugin": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^7.6.0",
    "@storybook/addon-actions": "^7.2.1",
    "@storybook/addon-storysource": "^7.2.1",
    "@storybook/react": "^7.2.1",
    "@storybook/theming": "^5.1.11",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "customize-cra": "^0.9.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^8.0.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "express": "^4.17.1",
    "file-loader": "^3.0.1",
    "git-branch-is": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "5.3.2",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.2",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "sass": "^1.51.0",
    "sass-loader": "^7.1.0",
    "standard-version": "^9.5.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^9.4.4",
    "typescript": "^4.9.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/trackolap/antrika-customer-portal.git"
  },
  "author": "Trackolap",
  "homepage": "https://gitlab.com/trackolap/antrika-customer-portal#readme",
  "license": "ISC",
  "private": true
}
